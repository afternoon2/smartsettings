{
    "source": "./flow-striped",
    "destination": "./docs",
    "excludes": [
        "\\.types\\.js$"
    ],
    "plugins": [
        {
            "name": "esdoc-standard-plugin",
            "option": {
                "accessor": {
                    "access": [
                        "public",
                        "protected",
                        "private"
                    ],
                    "autoPrivate": true
                }
            }
        },
        {
            "name": "esdoc-external-webapi-plugin",
            "option": {
                "enable": true
            }
        }
    ]
}