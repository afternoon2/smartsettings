{
  "name": "smartsettings",
  "description": "Yet another JavaScript library for creating (smart) setting panels",
  "version": "1.2.2",
  "license": "MIT",
  "author": "Jakub Antolak <poprostuantolak@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/afternoon2/smartsettings"
  },
  "browser": "dist/smartsettings.umd.js",
  "module": "dist/smartsettings.es.js",
  "devDependencies": {
    "autoprefixer": "^8.4.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.6.0",
    "commitizen": "^2.9.6",
    "cz-conventional-changelog": "^2.1.0",
    "jest": "^22.4.3",
    "jsdoc": "^3.5.5",
    "postcss-advanced-variables": "^2.3.3",
    "postcss-extend": "^1.0.5",
    "postcss-mixins": "^6.2.0",
    "postcss-nested": "^3.0.0",
    "rollup": "^0.58.2",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-cleanup": "^2.0.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-copy": "^0.2.3",
    "rollup-plugin-eslint": "^4.0.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-postcss": "^1.6.1",
    "rollup-plugin-serve": "^0.4.2",
    "rollup-plugin-uglify": "^3.0.0",
    "semantic-release": "^15.4.0",
    "travis-deploy-once": "^5.0.0"
  },
  "scripts": {
    "build": "rollup -c && npm run docs",
    "dev": "rollup -c && npm run docs",
